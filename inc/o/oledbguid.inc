'typedef unsigned long DBKIND;

' DBKINDENUM
%DBKIND_GUID_NAME    = 0
%DBKIND_GUID_PROPID  = %DBKIND_GUID_NAME    + 1
%DBKIND_NAME         = %DBKIND_GUID_PROPID  + 1
%DBKIND_PGUID_NAME   = %DBKIND_NAME         + 1
