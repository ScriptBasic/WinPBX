%DBKIND_PROPID       = %DBKIND_PGUID_PROPID + 1
%DBKIND_GUID         = %DBKIND_PROPID       + 1

'typedef struct tagDBID
'    {
'    /* [switch_is][switch_type] */ union
'        {
'        /* [case()] */ GUID guid;
'        /* [case()] */ GUID *pguid;
'        /* [default] */  /* Empty union arm */
'        }    uGuid;
'    DBKIND eKind;
'    /* [switch_is][switch_type] */ union
'        {
'        /* [case()] */ wchar_t* pwszName;
'        /* [case()] */ unsigned long ulPropid;
'        /* [default] */  /* Empty union arm */
'        }    uName;
'    }    DBID;

' // Size = 16 bytes
UNION DBID_GUID_UNION DWORD
   guid  AS GUID
   pguid AS GUID PTR
END UNION

' // Size = 8 bytes
UNION DBID_NAME_UNION DWORD
   pwszName AS WSTRINGZ PTR
   ulPropid AS DWORD
END UNION

' // Size = 24 bytes
TYPE DBID DWORD
   DBID_GUID_UNION
   eKind AS DWORD            ' // DBKIND Enum
   DBID_NAME_UNION
END TYPE

