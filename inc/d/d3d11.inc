'#define D3D11_ENCODE_BASIC_FILTER( min, mag, mip, bComparison )                                           \
'                                   ( ( D3D11_FILTER ) (                                                   \
'                                   ( ( bComparison ) ? D3D11_COMPARISON_FILTERING_BIT : 0 ) |             \
'                                   ( ( ( min ) & D3D11_FILTER_TYPE_MASK ) << D3D11_MIN_FILTER_SHIFT ) |   \
'                                   ( ( ( mag ) & D3D11_FILTER_TYPE_MASK ) << D3D11_MAG_FILTER_SHIFT ) |   \
'                                   ( ( ( mip ) & D3D11_FILTER_TYPE_MASK ) << D3D11_MIP_FILTER_SHIFT ) ) )

FUNCTION D3D11_ENCODE_BASIC_FILTER (BYVAL min_ AS DWORD, BYVAL mag_ AS DWORD, BYVAL mip_ AS DWORD, BYVAL bComparison AS LONG) AS DWORD
   LOCAL x0, x1, x2, x3 AS DWORD
   x0 = IIF(bComparison, %D3D11_COMPARISON_FILTERING_BIT, 0)
   x1 = min_ AND %D3D11_FILTER_TYPE_MASK
   SHIFT LEFT x1, %D3D11_MIN_FILTER_SHIFT
   x2 = mag_ AND %D3D11_FILTER_TYPE_MASK
   SHIFT LEFT x1, %D3D11_MAG_FILTER_SHIFT
   x3 = mip_ AND %D3D11_FILTER_TYPE_MASK
   SHIFT LEFT x1, %D3D11_MIP_FILTER_SHIFT
   FUNCTION = x0 OR x1 OR x2 OR x3
END FUNCTION

'#define D3D11_ENCODE_ANISOTROPIC_FILTER( bComparison )                                                \
'                                         ( ( D3D11_FILTER ) (                                         \
'                                         D3D11_ANISOTROPIC_FILTERING_BIT |                            \
'                                         D3D11_ENCODE_BASIC_FILTER( D3D11_FILTER_TYPE_LINEAR,         \
'                                                                    D3D11_FILTER_TYPE_LINEAR,         \
'                                                                    D3D11_FILTER_TYPE_LINEAR,         \
'                                                                    bComparison ) ) )

FUNCTION D3D11_ENCODE_ANISOTROPIC_FILTER (BYVAL bComparison AS LONG) AS DWORD
   FUNCTION = %D3D11_ANISOTROPIC_FILTERING_BIT OR D3D11_ENCODE_BASIC_FILTER(%D3D11_FILTER_TYPE_LINEAR, %D3D11_FILTER_TYPE_LINEAR, %D3D11_FILTER_TYPE_LINEAR, bComparison)
END FUNCTION

'#define D3D11_DECODE_MIN_FILTER( d3d11Filter )                                                              \
